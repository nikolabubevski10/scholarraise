    "d3-scale": "^3.2.2",
    "dayjs": "^1.8.16",
    "detect-browser": "^5.2.0",
    "formik": "^2.1.4",
    "html-react-parser": "^0.14.2",
    "humps": "^2.0.1",
    "is_js": "^0.9.0",
    "jsonp": "^0.2.1",
    "lodash.merge": "^4.6.2",
    "npm-run-all": "^4.1.5",
    "query-string": "6.8.3",
    "react": "^16.13.1",
    "react-browser-support": "^0.0.2",
    "react-countup": "4.2.2",
    "react-datepicker": "^3.3.0",
    "react-dom": "^16.13.1",
    "react-dropzone": "10.1.10",
    "react-fullstory": "^1.3.2",
    "react-ga": "^3.1.2",
